{
  "name": "vue3-element-template",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "cross-env NODE_ENV=production vite build --mode=production",
    "lint": "eslint src/**/*.{ts,tsx,vue,js,jsx}",
    "lint:fix": "eslint --fix src/**/*.{ts,tsx,vue,js,jsx}",
    "lint:style": "stylelint src/**/*.{css,scss,vue} --fix",
    "prettier": "prettier --write .",
    "type:check": "cross-env NODE_OPTIONS=--max-old-space-size=14096 vue-tsc --noEmit --skipLibCheck",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^1.7.3",
    "dayjs": "^1.11.12",
    "echarts": "^5.5.1",
    "echarts-gl": "^2.0.9",
    "echarts-wordcloud": "^2.1.0",
    "element-plus": "^2.8.0",
    "nprogress": "^0.2.0",
    "qrcode": "^1.5.4",
    "qs": "^6.13.0",
    "spark-md5": "^3.0.2",
    "swiper": "^11.1.9",
    "tinymce": "^7.3.0",
    "ts-md5": "^1.3.1",
    "vue": "^3.4.37",
    "vue-router": "^4.4.3",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/node": "^22.2.0",
    "@types/nprogress": "^0.2.3",
    "@types/qrcode": "^1.5.5",
    "@types/qs": "^6.9.15",
    "@types/spark-md5": "^3.0.4",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-unused-imports": "^4.1.3",
    "eslint-plugin-vue": "^9.27.0",
    "globals": "^15.9.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.8",
    "postcss": "^8.4.41",
    "postcss-html": "^1.7.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.3.3",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.77.8",
    "stylelint": "^16.8.1",
    "stylelint-config-property-sort-order-smacss": "^10.0.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.1",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.0.29"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "engines": {
    "node": ">= 18.18.0"
  }
}
